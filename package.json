{
  "name": "traverse",
  "author": "Jason Raimondi <jason@raimondi.us>",
  "description": "Traverse is a GitHub explorer. You can browse repositories trending by frequency and language.",
  "version": "0.11.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "publish": "bash publish.sh",
    "build": "run-s clean webpack:prod electron",
    "clean": "rm -rf dist/* dist-build/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "prestart": "run-s clean webpack:dev",
    "start": "run-p webpack:watch serve",
    "serve": "electron ./dist",
    "electron": "electron-builder --config electron-builder.yml",
    "electron:publish": "electron-builder --config electron-builder.yml -p always",
    "webpack:prod": "run-p webpack:main-prod webpack:renderer-prod",
    "webpack:dev": "run-p webpack:main-dev webpack:renderer-dev",
    "webpack:watch": "run-p webpack:main-watch webpack:renderer-watch",
    "webpack:main-watch": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --watch --config build/webpack.main.ts",
    "webpack:renderer-watch": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --watch --config build/webpack.renderer.ts",
    "webpack:main-dev": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config build/webpack.main.ts",
    "webpack:renderer-dev": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config build/webpack.renderer.ts",
    "webpack:main-prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json NODE_ENV=production webpack --config build/webpack.main.ts",
    "webpack:renderer-prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json NODE_ENV=production webpack --config build/webpack.renderer.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.26",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^4.4.3",
    "@types/webpack": "^4.41.21",
    "@types/webpack-merge": "^4.1.5",
    "axios": "^0.18.1",
    "axios-mock-adapter": "^1.18.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.1",
    "dayjs": "^1.8.29",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.8.0",
    "electron": "^11.5.0",
    "electron-builder": "^20.44.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-updater": "^4.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "faker": "^4.1.0",
    "file-loader": "^2.0.0",
    "formik": "^1.5.8",
    "html-webpack-plugin": "^3.2.0",
    "inversify": "^5.0.1",
    "jest": "^23.6.0",
    "lodash.findkey": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^5.1.2",
    "react-router-dom": "^4.3.1",
    "react-router-named-routes": "0.0.23",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-saga": "^0.16.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.7",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.9.6",
    "typescript-plugin-styled-components": "^1.4.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "analytics-node": "^3.3.0",
    "electron-settings": "^3.2.0",
    "uuid": "^3.3.2"
  }
}
